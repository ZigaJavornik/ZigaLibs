      <Declaration><![CDATA[METHOD Deactivate]]></Declaration>
      <Implementation>
        <ST><![CDATA[_active := FALSE;
]]></ST>
      </Implementation>
    </Method>
    <Method Name="SetInterval" Id="{49bc24d2-ba9f-0516-1ca8-4c245f14c997}">
      <Declaration><![CDATA[METHOD SetInterval : I_LightTowerComponent
VAR_INPUT
	tInterval : TIME;
END_VAR]]></Declaration>
