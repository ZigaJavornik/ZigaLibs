  <POU Name="FB_PwmLightTowerComponent" Id="{5c19a601-f767-0344-0722-2f67269d801d}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK INTERNAL FB_PwmLightTowerComponent IMPLEMENTS I_PwmLightTowerComponent, I_LightTowerComponent
VAR_INPUT
END_VAR
VAR_OUTPUT
END_VAR
VAR
	_active		: BOOL;
	_interval	: TIME;
	_state		: BOOL;
	fbPulseGenerator	: ZigaLibs_Core.FB_PulseGenerator( tPeriod := _interval, fDutyCycle := 50);
